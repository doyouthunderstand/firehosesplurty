<h1>Enter your quote details</h1>
<%= simple_form_for @quote do |f| %>
    <%= f.input :saying %>
    <%= f.input :author %>
    <div class="col-xs-6 required-field-warning">
        *Required Field
    </div>
    <br />
    <br />
    <%= f.submit 'Create' , class: 'btn btn-danger' %>
    </div>
<% end %>